<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.android.mymusic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LofiPlayList" android:label="@string/lofi_playlist" android:parentActivityName=".MainActivity"
            tools:targetApi="jelly_bean" />
        <activity android:name=".GamePlayList" android:label="@string/game_playlist" android:parentActivityName=".MainActivity"
            tools:targetApi="jelly_bean" />
        <activity android:name=".MusicPlayer" android:label="@string/now_playing" android:parentActivityName=".MainActivity"
            tools:targetApi="jelly_bean" />
        <activity android:name=".Favorites" android:label="@string/favorite" android:parentActivityName=".MainActivity"
            tools:targetApi="jelly_bean" />
        <activity android:name=".MyMusic" android:label="@string/my_music" android:parentActivityName=".MainActivity"
            tools:targetApi="jelly_bean" />
        <activity android:name=".Recent" android:label="@string/recent" android:parentActivityName=".MainActivity"
            tools:targetApi="jelly_bean" />
        <activity android:name=".MainActivity" android:label="@string/playlists">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>